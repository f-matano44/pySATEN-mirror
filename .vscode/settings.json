{
    "cSpell.words": [
        "pysaten",
        "SATEN",
    ],
    "black-formatter.args": [
        "--line-length",
        "79"
    ],
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "isort.args": [
        "--profile",
        "black"
    ],
    "editor.codeActionsOnSave": {
        "source.organizeImports": "always"
    },
    "flake8.args": [
        "--ignore=E203,W503,W504"
    ]
}
